{
  "name": "uitripled-monorepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "vercel-build": "turbo run build --filter=uitripled",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "registry:generate": "turbo run registry:generate",
    "registry:sync": "turbo run registry:sync",
    "registry:validate": "turbo run registry:validate",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build --filter=@uitripled/* && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.0",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": "^18.18.0 || ^20.0.0 || ^22.0.0"
  }
}
