{
  "name": "@uitripled/react-shadcn",
  "version": "0.1.0",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./src/*": "./src/*",
    "./components/*": {
      "types": "./dist/components/*.d.mts",
      "import": "./dist/components/*.mjs",
      "require": "./dist/components/*.js"
    },
    "./components/*/*": {
      "types": "./dist/components/*/*.d.mts",
      "import": "./dist/components/*/*.mjs",
      "require": "./dist/components/*/*.js"
    },
    "./components/*/*/*": {
      "types": "./dist/components/*/*/*.d.mts",
      "import": "./dist/components/*/*/*.mjs",
      "require": "./dist/components/*/*/*.js"
    },
    "./components/*/*/*/*": {
      "types": "./dist/components/*/*/*/*.d.mts",
      "import": "./dist/components/*/*/*/*.mjs",
      "require": "./dist/components/*/*/*/*.js"
    },
    "./components/*/*/*/*/*": {
      "types": "./dist/components/*/*/*/*/*.d.mts",
      "import": "./dist/components/*/*/*/*/*.mjs",
      "require": "./dist/components/*/*/*/*/*.js"
    },
    "./ui/*": {
      "types": "./dist/ui/*.d.mts",
      "import": "./dist/ui/*.mjs",
      "require": "./dist/ui/*.js"
    },
    "./styles/*": {
      "import": "./dist/styles/*.css"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.mts",
      "import": "./dist/lib/*.mjs",
      "require": "./dist/lib/*.js"
    },
    "./ui-library-provider": {
      "types": "./dist/ui-library-provider.d.mts",
      "import": "./dist/ui-library-provider.mjs",
      "require": "./dist/ui-library-provider.js"
    }
  },
  "scripts": {
    "build": "NODE_OPTIONS='--max-old-space-size=8192' tsup",
    "dev": "NODE_OPTIONS='--max-old-space-size=8192' tsup --watch",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "framer-motion": "^12.23.24",
    "@radix-ui/react-slot": "^1.2.3",
    "lucide-react": "^0.553.0",
    "class-variance-authority": "^0.7.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "recharts": "^3.4.1",
    "next-themes": "^0.4.6",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-tabs": "^1.1.13",
    "@uitripled/utils": "workspace:*"
  },
  "devDependencies": {
    "@uitripled/typescript-config": "workspace:*",
    "@uitripled/eslint-config": "workspace:*",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "next": "^16.0.0"
  }
}
